<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistroDos">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/RegistroDos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/registro2_texto"
            android:layout_width="272dp"
            android:layout_height="117dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="71dp"
            android:layout_marginEnd="10dp"
            android:text="@string/registro2_texto"
            android:textAlignment="center"
            android:textSize="50sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/registro2_texto_clave1"
            android:layout_width="240dp"
            android:layout_height="75dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="18dp"
            android:text="@string/registro2_texto_clave1"
            android:textAlignment="center"
            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/registro2_long_clave"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/registro2_texto" />

        <EditText
            android:id="@+id/registro2_clave1"
            android:layout_width="305dp"
            android:layout_height="57dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="18dp"
            android:text="@string/registro2_hint_clave1"
            app:layout_constraintBottom_toTopOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.257"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/registro2_imagen_ver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="7dp"
            android:layout_marginEnd="14dp"
            android:layout_marginBottom="383dp"
            android:background="@android:color/background_light"
            android:onClick="password_visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/registro2_clave1"
            app:srcCompat="@android:drawable/ic_menu_view" />

        <TextView
            android:id="@+id/registro2_long_clave2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="326dp"
            android:layout_marginEnd="69dp"
            android:text="@string/registro2_long_clave"
            app:layout_constraintBottom_toTopOf="@+id/registro2_clave2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="247dp"
            android:layout_height="83dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="25dp"
            android:text="@string/registro2_texto_clave2"
            android:textAlignment="center"
            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/registro2_clave2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/registro2_clave2"
            android:layout_width="305dp"
            android:layout_height="57dp"
            android:layout_marginStart="36dp"
            android:layout_marginBottom="62dp"
            android:text="@string/registro2_hint_clave1"
            app:layout_constraintBottom_toTopOf="@+id/registro2_confirmar"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/registro2_imagen_ver2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="7dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="62dp"
            android:background="@android:color/background_light"
            android:onClick="password2_visible"
            app:layout_constraintBottom_toTopOf="@+id/registro2_confirmar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.818"
            app:layout_constraintStart_toEndOf="@+id/registro2_clave2"
            app:srcCompat="@android:drawable/ic_menu_view" />

        <TextView
            android:id="@+id/registro2_long_clave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="326dp"
            android:layout_marginEnd="69dp"
            android:text="@string/registro2_long_clave"
            app:layout_constraintBottom_toTopOf="@+id/registro2_clave1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/registro2_confirmar"
            android:layout_width="145dp"
            android:layout_height="54dp"
            android:layout_marginStart="217dp"
            android:layout_marginEnd="49dp"
            android:layout_marginBottom="80dp"
            android:onClick="registrar"
            android:text="@string/registro2_confirmar"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>